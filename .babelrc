{
  "presets": ["react", "es2015"],
  "compact": false,
  "plugins": [
    ["transform-class-properties"],
    [
      "css-modules-transform", {
        "preprocessCss": "./lib/processSass.js",
        "extensions": [".css", ".scss"]
      }
    ]
  ],
  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  }
}
