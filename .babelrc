{
  "presets": ["react", "es2015"],
  "compact": false,
  "plugins": [
    ["transform-class-properties"]
  ],
  "env": {
    "development": {
      "plugins": [["react-transform", {
        "transforms": [{
          "transform": "react-transform-hmr",
          "imports": ["react"],
          "locals": ["module"]
        }]
      }]]
    }
  }
}
