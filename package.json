{
  "name": "express-react-webpack-babel",
  "version": "1.0.0",
  "author": "Nattajak Grisiam",
  "scripts": {
    "react-dev": "SET NODE_ENV=development&& webpack-dev-server --hot --inline --content-base src/static --port 3001",
    "dev": "SET NODE_ENV=development && SET PORT=3000 && nodemon --exec babel-node src/server.js",
    "prestart": "babel src --out-dir dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.5",
    "connect-mongo": "^1.3.2",
    "ejs": "^2.5.3",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-session": "^1.14.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.7.1",
    "morgan": "^1.7.0",
    "node-localstorage": "^1.3.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "webpack": "^1.13.3"
  }
}
