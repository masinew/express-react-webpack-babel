{
  "name": "express-react-webpack-babel",
  "version": "1.0.0",
  "author": "Nattajak Grisiam",
  "scripts": {
    "windows-dev": "\"node_modules\\.bin\\npm-run-all\" --parallel windows-client-dev windows-server-dev",
    "windows-client-dev": "SET BABEL_ENV=client-dev& \"node_modules\\.bin\\webpack-dev-server\" --config src/client/webpack.client.config.js",
    "windows-server-dev": "\"node_modules\\.bin\\npm-run-all\" --parallel windows-server-webpack-dev windows-server-start-dev",
    "windows-server-webpack-dev": "SET BABEL_ENV=server-dev&& \"node_modules\\.bin\\webpack\" --config src/server/webpack.server.config.js",
    "windows-server-start-dev": "SET BABEL_ENV=server-dev&& SET NODE_ENV=development && SET PORT=3000 && \"node_modules\\.bin\\nodemon\" src/server/server-app.js",
    "windows-server-dev-tmp": "SET NODE_ENV=development && SET PORT=4000 && nodemon --exec \"node_modules\\.bin\\babel-node\" src/server/server-app.js",
    "prestart": "SET BABEL_ENV=production&& babel src --out-dir dist&&\"node_modules\\.bin\\webpack\"",
    "start": "node dist/server/server.js"
  },
  "dependencies": {
    "alertifyjs": "^1.8.0",
    "body-parser": "^1.15.2",
    "bootstrap": "^4.0.0-alpha.5",
    "connect-mongo": "^1.3.2",
    "ejs": "^2.5.3",
    "es6-promise": "^4.0.5",
    "express": "^4.14.0",
    "express-minify-html": "^0.9.0",
    "express-session": "^1.14.2",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.1.9",
    "mongoose": "^4.7.1",
    "morgan": "^1.7.0",
    "multer": "^1.2.1",
    "node-localstorage": "^1.3.0",
    "npm-run-all": "^3.1.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel": "^6.5.2",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-css-modules-transform": "^1.1.0",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "jquery": "^3.1.1",
    "less": "^2.7.1",
    "less-loader": "^2.2.3",
    "node-sass": "^4.1.0",
    "nodemon": "^1.11.0",
    "npm-install-webpack-plugin": "^4.0.4",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.2.1",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-transform-hmr": "^1.0.4",
    "sass-loader": "^4.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.1.2"
  }
}
