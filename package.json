{
  "name": "express-react-webpack-babel",
  "version": "1.0.0",
  "author": "Nattajak Grisiam",
  "scripts": {
    "react-dev": "webpack-dev-server --hot --inline --content-base src/static --port 3000",
    "dev": "SET NODE_ENV=development && SET PORT=3000 && nodemon --exec babel-node src/server.js",
    "prestart": "babel src --out-dir dist",
    "start": "node dist/server.js"
  },
  "dependencies": {
    "bootstrap": "^4.0.0-alpha.5",
    "ejs": "^2.5.3",
    "express": "^4.14.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-router": "^3.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "webpack": "^1.13.3"
  }
}
